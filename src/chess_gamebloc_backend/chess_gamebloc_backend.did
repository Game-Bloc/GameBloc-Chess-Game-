type ManualReply = record {
  name : text;
  description : text;
  keywords : vec text;
};
type Profile = record { 
    name : text; 
    description : text; 
    keywords : vec text;
};
service : {
  get : (text) -> (ManualReply) query;
  getSelf : () -> (ManualReply) query;
  search : (text) -> (opt Profile) query;
  update : (Profile) -> ();
}
